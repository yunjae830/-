<configuration>
	<typeAliases>
		<typeAlias type="com.tmail.board.Dto.AddressDto" alias="AddressDto"/>
		<typeAlias type="com.tmail.board.Dto.RegisterDto" alias="RegisterDto"/>
		<typeAlias type="com.tmail.board.Dto.MailboxDto" alias="MailboxDto"/>
